{
  "name": "sensei-uaepass-workspace",
  "version": "1.0.0",
  "description": "ðŸ¥‹ Sensei UAE Pass - Master of UAE Pass Integration workspace",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo",
    "start:demo": "ng serve demo --open",
    "start:proxy": "node proxy-server.js",
    "start:full": "concurrently \"npm run start:proxy\" \"npm run start:demo\"",
    "build": "ng build",
    "build:lib": "ng build uae-pass",
    "build:demo": "ng build demo --configuration production",
    "build:watch": "ng build uae-pass --watch --configuration development",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "pack:lib": "cd dist/uae-pass && npm pack",
    "publish:lib": "cd dist/uae-pass && npm publish --access public",
    "release": "npm run build:lib && npm run pack:lib",
    "dev": "npm run start:full",
    "docs:serve": "cd docs && python -m http.server 8080",
    "clean": "rimraf dist node_modules/.cache",
    "version:patch": "cd projects/uae-pass && npm version patch --no-git-tag-version",
    "version:minor": "cd projects/uae-pass && npm version minor --no-git-tag-version",
    "version:major": "cd projects/uae-pass && npm version major --no-git-tag-version",
    "prepare:release": "npm run clean && npm run build:lib && npm run pack:lib",
    "docker:backend": "cd examples/backends/nodejs-express && docker build -t sensei-uaepass-backend .",
    "docker:run": "docker run -p 3001:3001 sensei-uaepass-backend"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.8",
    "@angular/cli": "^19.2.8",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "node-fetch": "^2.7.0",
    "typescript": "~5.7.2"
  }
}
